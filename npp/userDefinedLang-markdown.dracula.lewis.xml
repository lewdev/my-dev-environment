<NotepadPlus>
    <UserLang name="Markdown (Dracula)" ext="md markdown" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="2" decimalSeparator="0" />
            <Prefix Keywords1="yes" Keywords2="yes" Keywords3="yes" Keywords4="yes" Keywords5="yes" Keywords6="yes" Keywords7="yes" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00# 01 02((EOL)) 03&lt;!-- 04--&gt;</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1">.</Keywords>
            <Keywords name="Numbers, suffix2">.</Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">@ &lt; &gt; \&lt; \&gt; \\ \` \* \_ \{ \} \[ \] \( \) \# \+ \- \. \! \~ :|: |: :| | \|</Keywords>
            <Keywords name="Operators2">- +</Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">http:// (http:// https:// (https:// mailto: (mailto: ftp:// (ftp:// ftps:// (ftps:// (/ /</Keywords>
            <Keywords name="Keywords2">==== ----</Keywords>
            <Keywords name="Keywords3">*** ___</Keywords>
            <Keywords name="Keywords4">** __</Keywords>
            <Keywords name="Keywords5">* _</Keywords>
            <Keywords name="Keywords6">** __</Keywords>
            <Keywords name="Keywords7">* _</Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00![ 00[ 01\ 02] 02] 03``` 03` 03~~~ 04\ 05``` 05((EOL `)) 05~~~ 06*** 07\ 08((EOL ***)) 09** 10\ 11((EOL **)) 12* 13\ 14((EOL *)) 15** 16\ 17((EOL **)) 18* 19\ 20((EOL *)) 21 22 23</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="C0C0C0" bgColor="282A36" fontName="no" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="FFFF80" bgColor="282A36" fontName="&#x3978;A&#x7F00;" fontStyle="2" nesting="0" />
            <WordsStyle name="LINE COMMENTS" fgColor="FF8000" bgColor="282A36" fontName="&#x0F6E;&#x88AB;&#x05B9;&#x8800;&#x8E60;&#xA261;&#x0157;" fontStyle="1" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="0080FF" bgColor="282A36" fontName="&#x0001;&#x4E2E;&#x4B41;&#x4800;&#x0099;" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="C88FFF" bgColor="282A36" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="FF8000" bgColor="282A36" fontName="yes" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="000080" bgColor="282A36" fontName="0" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="0080C0" bgColor="282A36" fontName="&#x609B;&#x88DF;&#x2200;&#x9000;titleBar" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="0080C0" bgColor="282A36" fontName="&#x60EF;&#x88A3;&#x0600;&#x9000;GUIConfig" fontStyle="2" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="0080C0" bgColor="282A36" fontName="&#x60C4;&#x889E;&#x0D00;&#x9000;sessionExt" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="0080C0" bgColor="282A36" fontName="&#x0001;" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS8" fgColor="FFFFFF" bgColor="282A36" fontName="0" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" fgColor="8080FF" bgColor="282A36" fontName="0" fontStyle="1" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" fgColor="8080C0" bgColor="282A36" fontName="no" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="80FFFF" bgColor="282A36" fontName="&#x7700;&#xA257;&#x0157;" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="FF8040" bgColor="282A36" fontName="41" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="C88FFF" bgColor="282A36" fontName="aligned" fontStyle="2" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="008000" bgColor="282A36" fontName="hide" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS3" fgColor="0080C0" bgColor="282A36" fontName="box" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS4" fgColor="0080C0" bgColor="282A36" fontName="no" fontStyle="1" nesting="65600" />
            <WordsStyle name="DELIMITERS5" fgColor="0080C0" bgColor="282A36" fontName="show" fontStyle="2" nesting="32800" />
            <WordsStyle name="DELIMITERS6" fgColor="0080C0" bgColor="282A36" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS7" fgColor="0080C0" bgColor="282A36" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="DELIMITERS8" fgColor="0080FF" bgColor="282A36" fontName="er" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
